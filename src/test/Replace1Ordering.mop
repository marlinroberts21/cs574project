Replace1Ordering(Replace1 r){
    event op_before before(Replace1 r, Automaton a) :
          call(public Automaton Replace1.op(Automaton)) && target(r) && args(a) {}
    event op_after after(Replace1 r, Automaton a) :
          call(public Automaton Replace1.op(Automaton)) && target(r) && args(a) {}

    ere : (op_before op_after)*

    @fail{
        System.err.println("!MOP Error! Somehow you done messed up the operation order");
        __RESET;
    }
}
